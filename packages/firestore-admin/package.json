{
  "name": "@forest-fire/firestore-admin",
  "version": "0.59.6",
  "description": "Admin SDK for the Firestore database",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/universal-fire.git"
  },
  "scripts": {
    "clean": "rimraf dist/**/*",
    "watch": "run-p 'watch:esnext' 'watch:cjs'",
    "watch:esnext": "tsc -P tsconfig.es.json -w",
    "watch:cjs": "tsc -P tsconfig.es.json -w",
    "test": "yarn dd test",
    "test:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files src src/**/*-spec.ts",
    "test:coverage": "nyc --reporter=text mocha -require ts-node/register src/**/*-spec.ts",
    "test:e2e": "mocha --require ts-node/register test/*-e2e.ts"
  },
  "peerDependencies": {
    "firebase-admin": ">= 9.4.1"
  },
  "optionalDependencies": {
    "firemock": "^0.59.6"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "dd": "^0.11.3",
    "firebase-admin": "9.4.1",
    "firemock": "^0.59.2",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@forest-fire/abstracted-database": "^0.59.6",
    "@forest-fire/firestore-db": "^0.59.6",
    "@forest-fire/types": "^0.59.6",
    "@forest-fire/utility": "^0.59.6"
  },
  "license": "MIT",
  "gitHead": "3f16ea360707b8579b4d09453465153be4d2b1eb"
}
