{
  "name": "firemock",
  "version": "0.52.7",
  "description": "firemock",
  "license": "MIT",
  "repository": "git@github.com:forest-fire/firemock.git",
  "author": "Ken Snyder<ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase",
    "mocking",
    "testing"
  ],
  "files": [
    "dist"
  ],
  "module": "dist/es/index.js",
  "esnext": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "main": "dist/cjs/index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist/**/*",
    "build": "run-s clean build:all",
    "build:all": "run-p build:esnext build:cjs",
    "build:esnext": "tsc -P tsconfig.es.json",
    "build:cjs": "tsc -P tsconfig.cjs.json",
    "test": "yarn do test",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "watch": "ts-node ./scripts/watch.ts",
    "watch:test": "ts-node ./scripts/watch.ts test",
    "watch:all": "ts-node ./scripts/watch.ts all",
    "deploy": "ts-node ./scripts/deploy.ts ",
    "info": "yarn do info",
    "circular": "yarn run madge --circular --extensions ts ./src"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/deepmerge": "^2.2.0",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash.first": "^3.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.last": "^3.0.6",
    "@types/lodash.set": "^4.3.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^8.0.0",
    "@types/stack-trace": "^0.0.29",
    "@vuepress/plugin-back-to-top": "^1.4.1",
    "@vuepress/plugin-last-updated": "^1.4.1",
    "@vuepress/plugin-medium-zoom": "^1.4.1",
    "@vuepress/plugin-pwa": "^1.4.1",
    "async-shelljs": "^0.1.4",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "do-devops": "^0.3.5",
    "madge": "^3.8.0",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "test-console": "^1.1.0",
    "ts-node": "^8.9.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "vuepress": "^1.4.1"
  },
  "dependencies": {
    "@forest-fire/serialized-query": "^0.52.7",
    "@forest-fire/types": "^0.52.0",
    "@types/faker": "^4.1.12",
    "common-types": "^1.14.3",
    "deepmerge": "^4.2.2",
    "email-validator": "^2.0.4",
    "faker": "^4.1.0",
    "fast-copy": "^2.0.4",
    "fast-equals": "^2.0.0",
    "firebase-key": "^2.0.2",
    "lodash.first": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.last": "^3.0.0",
    "lodash.set": "^4.3.2",
    "typed-conversions": "^0.10.4"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
