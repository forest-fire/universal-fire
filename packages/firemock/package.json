{
  "name": "firemock",
  "version": "0.53.28",
  "description": "firemock",
  "license": "MIT",
  "repository": "git@github.com:forest-fire/firemock.git",
  "author": "Ken Snyder<ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase",
    "mocking",
    "testing"
  ],
  "files": [
    "dist"
  ],
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "main": "dist/cjs/index.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist/**/*",
    "clean:nuclear": "rimraf dist node_modules && yarn cache clean && rm yarn.lock && yarn && yarn upgrade",
    "build": "run-s clean build:bundle",
    "build:bundle": "rollup -c",
    "build:all": "run-p build:esnext build:cjs",
    "build:esnext": "ttsc -P tsconfig.es.json",
    "build:cjs": "ttsc -P tsconfig.cjs.json",
    "test": "yarn jest",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "watch": "ts-node ./scripts/watch.ts",
    "watch:test": "ts-node ./scripts/watch.ts test",
    "watch:all": "ts-node ./scripts/watch.ts all",
    "deploy": "ts-node ./scripts/deploy.ts ",
    "info": "yarn do info",
    "autoindex": "yarn do autoindex",
    "circular": "yarn run madge --circular --extensions ts ./src"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/deepmerge": "^2.2.0",
    "@types/jest": "^26.0.3",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash.first": "^3.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.last": "^3.0.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^8.0.0",
    "@types/stack-trace": "^0.0.29",
    "@vuepress/plugin-back-to-top": "^1.5.0",
    "@vuepress/plugin-last-updated": "^1.5.0",
    "@vuepress/plugin-medium-zoom": "^1.5.0",
    "@vuepress/plugin-pwa": "^1.5.0",
    "async-shelljs": "^0.1.4",
    "chalk": "^4.0.0",
    "do-devops": "^0.3.20",
    "jest": "^26.1.0",
    "jest-extended": "^0.11.5",
    "lodash": "^4.17.15",
    "madge": "^3.9.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "test-console": "^1.1.0",
    "ts-babel": "^6.1.7",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5",
    "typescript-transform-paths": "^1.1.14",
    "vuepress": "^1.5.0"
  },
  "dependencies": {
    "@forest-fire/serialized-query": "^0.53.28",
    "@forest-fire/types": "^0.53.25",
    "@types/faker": "^4.1.12",
    "common-types": "^1.15.8",
    "deepmerge": "^4.2.2",
    "email-validator": "^2.0.4",
    "faker": "^4.1.0",
    "fast-copy": "^2.1.0",
    "fast-equals": "^2.0.0",
    "firebase-key": "^2.0.2",
    "lodash.first": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.last": "^3.0.0",
    "lodash.set": "^4.3.2",
    "typed-conversions": "^0.11.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
