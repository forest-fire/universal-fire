  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean build:all",
    "build:all": "run-p build:esnext build:cjs",
    "build:esnext": "tsc -P tsconfig.es.json",
    "build:cjs": "tsc -P tsconfig.cjs.json",
    "watch": "run-p watch:esnext watch:cjs",
    "watch:esnext": "tsc -P tsconfig.es.json -w",
    "watch:cjs": "tsc -P tsconfig.es.json -w",
    "test": "TS_NODE_PROJECT='tsconfig.test.json' mocha --require ts-node/register test/**/*.spec.ts",
    "test:watch": "TS_NODE_PROJECT='tsconfig.test.json' mocha --require ts-node/register --extensions ts --watch --watch-files test test/**/*.spec.ts",
    "test:coverage": "TS_NODE_PROJECT='tsconfig.test.json' nyc --reporter=text mocha -require ts-node/register test/**/*.spec.ts",
    "test:e2e": "TS_NODE_PROJECT='tsconfig.test.json' mocha --require ts-node/register test/*.e2e.ts"
  },
  "dependencies": {
    "common-types": "^1.14.3"
  },
