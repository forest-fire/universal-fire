{
  "name": "abstracted-database",
  "version": "0.3.0",
  "description": "",
  "main": "dist/commonjs",
  "module": "dist/esnext",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/sdk.git"
  },
  "scripts": {
    "build": "rimraf dist/ && tsc --module commonjs --outDir dist/commonjs && tsc --module esnext --outDir dist/esnext",
    "test": "mocha --require ts-node/register src/**/*.spec.ts",
    "test:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files src src/**/*.ts",
    "test:coverage": "nyc --reporter=text mocha -require ts-node/register src/**/*.spec.ts",
    "test:e2e": "mocha --require ts-node/register test/*.e2e.ts"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.6.0",
    "@firebase/database-types": "^0.4.14",
    "@firebase/firestore-types": "^1.10.1"
  },
  "dependencies": {
    "@firebase/auth": "^0.14.1",
    "firemock": "^0.37.5"
  },
  "license": "MIT"
}
