{
  "name": "universal-fire",
  "version": "0.50.8",
  "description": "Provides a static initializer to all of the Universal Fire SDK's",
  "module": "dist/es",
  "types": "dist/es/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/universal-fire.git"
  },
  "scripts": {
    "clean": "rimraf dist/**/*",
    "build": "run-s clean build:all",
    "build:all": "run-p build:esnext build:cjs",
    "build:esnext": "tsc -P tsconfig.es.json",
    "build:cjs": "echo no CJS for static initializer",
    "build:monorepo": "cd ../../ && yarn build && cd -",
    "watch": "run-p watch:esnext watch:cjs",
    "watch:esnext": "echo not implemented",
    "watch:cjs": "echo not implemented",
    "test": "yarn do test",
    "test:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files src src/**/*.ts",
    "test:coverage": "nyc --reporter=text mocha -require ts-node/register src/**/*.spec.ts",
    "test:e2e": "mocha --require ts-node/register test/*.e2e.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "do-devops": "^0.3.8",
    "mocha": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.11.2",
    "rollup-plugin-terser": "^6.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@forest-fire/abstracted-database": "^0.50.8",
    "@forest-fire/base-serializer": "^0.50.8",
    "@forest-fire/firestore-client": "^0.50.8",
    "@forest-fire/firestore-db": "^0.50.8",
    "@forest-fire/real-time-client": "^0.50.8",
    "@forest-fire/real-time-db": "^0.50.8",
    "@forest-fire/serialized-query": "^0.50.8",
    "@forest-fire/types": "^0.50.1",
    "common-types": "^1.14.7"
  },
  "license": "MIT"
}
