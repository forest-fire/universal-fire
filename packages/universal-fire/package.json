{
  "name": "universal-fire",
  "version": "0.53.1",
  "description": "Provides a static initializer to all of the Universal Fire SDK's",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/src/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/universal-fire.git"
  },
  "scripts": {
    "clean": "rimraf dist/**/*",
    "build": "run-s clean build:all",
    "build:all": "rollup -c",
    "build:monorepo": "cd ../../ && yarn build && cd -",
    "watch": "run-p watch:esnext watch:cjs",
    "watch:esnext": "echo not implemented",
    "watch:cjs": "echo not implemented",
    "test": "yarn do test",
    "test:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files src src/**/*.ts",
    "test:coverage": "nyc --reporter=text mocha -require ts-node/register src/**/*.spec.ts",
    "test:e2e": "mocha --require ts-node/register test/*.e2e.ts"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "do-devops": "^0.3.18",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.11.2",
    "rollup-plugin-terser": "^6.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@forest-fire/abstracted-database": "^0.53.1",
    "@forest-fire/firestore-admin": "^0.53.1",
    "@forest-fire/firestore-client": "^0.53.1",
    "@forest-fire/firestore-db": "^0.53.1",
    "@forest-fire/real-time-admin": "^0.53.1",
    "@forest-fire/real-time-client": "^0.53.1",
    "@forest-fire/real-time-db": "^0.53.1",
    "@forest-fire/serialized-query": "^0.53.1",
    "@forest-fire/serializer-factory": "^0.53.1",
    "@forest-fire/serilizer-factory": "^0.52.4",
    "@forest-fire/types": "^0.53.0",
    "common-types": "^1.15.6"
  },
  "license": "MIT"
}
