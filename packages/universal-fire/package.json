{
  "name": "universal-fire",
  "version": "0.55.3",
  "description": "Provides a static initializer to all of the Universal Fire SDK's",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "browser": "dist/es/browser.js",
  "types": "dist/types/index.node.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/universal-fire.git"
  },
  "scripts": {
    "clean": "rimraf dist/**/*",
    "watch": "run-p watch:esnext watch:cjs",
    "watch:esnext": "echo not implemented",
    "watch:cjs": "echo not implemented",
    "test": "yarn do test",
    "test:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files src src/**/*.ts",
    "test:coverage": "nyc --reporter=text mocha -require ts-node/register src/**/*.spec.ts",
    "test:e2e": "mocha --require ts-node/register test/*.e2e.ts"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "do-devops": "^0.3.20",
    "firemock": "^0.55.3",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@forest-fire/firestore-admin": "^0.55.3",
    "@forest-fire/firestore-client": "^0.55.3",
    "@forest-fire/real-time-admin": "^0.55.3",
    "@forest-fire/real-time-client": "^0.55.3",
    "@forest-fire/serialized-query": "^0.55.3",
    "@forest-fire/serializer-factory": "^0.55.3",
    "@forest-fire/types": "^0.55.3",
    "common-types": "^1.15.8"
  },
  "license": "MIT"
}
