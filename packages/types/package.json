{
  "name": "@forest-fire/types",
  "version": "0.45.1",
  "description": "@forest-fire/* Types",
  "module": "dist/es/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/es/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/universal-fire.git"
  },
  "scripts": {
    "clean": "rimraf dist/**/*",
    "build": "run-s clean build:all",
    "build:all": "run-p build:esnext build:cjs",
    "build:esnext": "tsc -P tsconfig.es.json",
    "build:cjs": "tsc -P tsconfig.cjs.json",
    "watch": "run-p 'watch:esnext' 'watch:cjs'",
    "watch:esnext": "tsc -P tsconfig.es.json ",
    "watch:cjs": "tsc -P tsconfig.es.json "
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@firebase/app-types": "^0.6.0",
    "@firebase/auth-types": "^0.10.0",
    "@firebase/database-types": "^0.5.0",
    "@firebase/firestore-types": "^1.10.1",
    "@types/faker": "^4.1.12",
    "common-types": "^1.14.3",
    "faker": "^4.1.0",
    "firebase-admin": "^8.12.1"
  },
  "license": "UNLICENSED"
}
