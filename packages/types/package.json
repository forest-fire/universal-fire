{
  "name": "@forest-fire/types",
  "version": "0.55.0",
  "description": "@forest-fire/* Types",
  "module": "dist/es/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forest-fire/universal-fire.git"
  },
  "scripts": {
    "clean": "rimraf dist/**/*",
    "build": "run-s clean build:bundle",
    "build:bundle": "rollup -c",
    "build:all": "run-p build:esnext build:cjs",
    "build:esnext": "tsc -P tsconfig.es.json",
    "build:cjs": "tsc -P tsconfig.cjs.json",
    "watch": "run-p 'watch:esnext' 'watch:cjs'",
    "watch:esnext": "tsc -P tsconfig.es.json ",
    "watch:cjs": "tsc -P tsconfig.es.json "
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "do-devops": "^0.3.20",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-node": "^8.10.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@firebase/app-types": "^0.6.1",
    "@firebase/auth-types": "^0.10.1",
    "@firebase/database-types": "^0.5.1",
    "@firebase/firestore-types": "^1.11.0",
    "@types/faker": "^4.1.12",
    "common-types": "^1.15.8",
    "faker": "^4.1.0",
    "firebase-admin": "^8.13.0"
  },
  "license": "MIT"
}
